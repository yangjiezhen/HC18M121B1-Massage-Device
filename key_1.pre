void key_tick(void)
{
static unsigned char stable_cnt = 0;
static unsigned char last_key = 0;
static unsigned char key_released = 1;

key_scan();

if (key == last_key)
{

if (stable_cnt < 40)
stable_cnt++;
else
{

if (key != 0 && key_released == 1)
{
final_key = key;
key_released = 0;
}

else if (key == 0)
{
final_key = 0;
key_released = 1;
}
}
}
else
{

stable_cnt = 0;
last_key = key;
}
}

