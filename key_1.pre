void key_scan(void)
{

key=0;
if(key_mode==0)
{
keymode0_configuration();
DelayUs2x(10);
if(PORTA7==0)
{
key=10;key_mode=0;return;
}
if(PORTA6==0)
{
key=12;key_mode=0;return;
}
if(PORTA4==0)
{
key=5;key_mode=0;return;
}
else
{
key_mode=1;
}
}
if(key_mode==1)
{
keymode1_configuration();
DelayUs2x(10);
if(PORTA7==1)
{
key=1;key_mode=0;return;
}
else
{
key_mode=2;
}
}
if(key_mode==2)
{
keymode3_configuration();
DelayUs2x(10);

if (PORTA4==1)
{
key=2;key_mode=0;return;
}
else
{
key_mode=3;
}

}
